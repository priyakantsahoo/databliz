# Datablize Environment Configuration
# Copy this file to .env for local development

# Application Configuration
VITE_APP_NAME=Datablize
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION="AI-Powered Document Intelligence for the Modern Enterprise"

# Build Configuration
NODE_ENV=production
VITE_BUILD_TIME=auto

# Contact Configuration  
VITE_SALES_EMAIL=sales@databliz.com
VITE_SUPPORT_EMAIL=support@databliz.com

# Analytics (Optional - Add your tracking IDs)
# VITE_GA_TRACKING_ID=
# VITE_HOTJAR_ID=

# Feature Flags (Optional)
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_CHAT_WIDGET=false

# Domain Configuration (for production)
VITE_APP_URL=https://datablize.com